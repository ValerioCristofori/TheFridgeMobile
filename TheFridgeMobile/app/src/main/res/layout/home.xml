<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/clTop"        android:layout_width="match_parent"        android:layout_height="40dp"        android:background="@color/myDarkBlue"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <TextView            android:id="@+id/tvAboutUs"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="20dp"            android:fontFamily="@font/comfortaa"            android:text="@string/your_fridge"            android:textStyle="bold"            android:textColor="@color/myWhite"            android:textSize="18sp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/clCenter"        android:layout_width="match_parent"        android:layout_height="0dp"        android:background="@color/myColorForBackground"        app:layout_constraintBottom_toTopOf="@+id/clBottom"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/clTop">        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/viewSearchBar"            android:layout_width="249dp"            android:layout_height="35dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <EditText                android:id="@+id/etSearchBar"                android:layout_width="224dp"                android:layout_height="36dp"                android:ems="10"                android:fontFamily="@font/comfortaa"                android:hint="@string/search_food"                android:inputType="textPersonName"                android:textColor="@color/myWhite"                android:textColorHint="@color/myGrey"                android:textSize="12sp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent"                app:layout_constraintVertical_bias="0"                android:importantForAutofill="no" />            <ImageView                android:id="@+id/ivSearch"                android:layout_width="22dp"                android:layout_height="23dp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toStartOf="@+id/etSearchBar"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent"                app:srcCompat="@drawable/ic_youtube_searched_for_black_24dp"                tools:ignore="ContentDescription" />        </androidx.constraintlayout.widget.ConstraintLayout>        <androidx.recyclerview.widget.RecyclerView            android:id="@+id/rvFridge"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_marginTop="40dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/clBottom"        android:layout_width="match_parent"        android:layout_height="60dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        android:background="@color/myDarkBlue"        app:layout_constraintHorizontal_bias="1.0"        app:layout_constraintStart_toStartOf="parent">        <TextView            android:id="@+id/textView5"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="28dp"            android:layout_marginBottom="4dp"            android:fontFamily="@font/comfortaa"            android:text="@string/your_fridge"            android:textSize="@dimen/dimension_text_title_menu"            android:textStyle="bold"            android:textColor="@color/myWhite"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent" />        <TextView            android:id="@+id/textView2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginBottom="4dp"            android:fontFamily="@font/comfortaa"            android:text="@string/recipes"            android:textSize="@dimen/dimension_text_title_menu"            android:textStyle="bold"            android:textColor="@color/myGrey"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/textView3"            app:layout_constraintHorizontal_bias="0.690"            app:layout_constraintStart_toEndOf="@+id/textView5" />        <TextView            android:id="@+id/textView3"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginEnd="36dp"            android:layout_marginBottom="4dp"            android:fontFamily="@font/comfortaa"            android:textStyle="bold"            android:textColor="@color/myGrey"            android:text="@string/profile"            android:textSize="@dimen/dimension_text_title_menu"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent" />        <TextView            android:id="@+id/textView4"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginBottom="4dp"            android:fontFamily="@font/comfortaa"            android:text="@string/add_food"            android:textSize="@dimen/dimension_text_title_menu"            android:textStyle="bold"            android:textColor="@color/myGrey"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/textView2"            app:layout_constraintHorizontal_bias="0.470"            app:layout_constraintStart_toEndOf="@+id/textView5" />        <ImageButton            android:id="@+id/ibtnFridge"            android:layout_width="40dp"            android:layout_height="40dp"            android:layout_marginStart="30dp"            android:background="@android:color/transparent"            android:src="@drawable/ic_fridge_white"            app:layout_constraintBottom_toTopOf="@+id/textView5"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            tools:ignore="ContentDescription" />        <ImageButton            android:id="@+id/ibtnAddFood"            android:layout_width="40dp"            android:layout_height="40dp"            android:background="@android:color/transparent"            android:src="@drawable/ic_supermarket_grey"            app:layout_constraintBottom_toTopOf="@+id/textView4"            app:layout_constraintEnd_toStartOf="@+id/ibtnRecipes"            app:layout_constraintStart_toEndOf="@+id/ibtnFridge"            app:layout_constraintTop_toTopOf="parent"            tools:ignore="ContentDescription" />        <ImageButton            android:id="@+id/ibtnRecipes"            android:layout_width="40dp"            android:layout_height="40dp"            android:background="@android:color/transparent"            android:src="@drawable/ic_chef_grey"            app:layout_constraintBottom_toTopOf="@+id/textView2"            app:layout_constraintEnd_toStartOf="@+id/ibtnProfile"            app:layout_constraintHorizontal_bias="0.717"            app:layout_constraintStart_toEndOf="@+id/ibtnFridge"            app:layout_constraintTop_toTopOf="parent"            tools:ignore="ContentDescription" />        <ImageButton            android:id="@+id/ibtnProfile"            android:layout_width="40dp"            android:layout_height="40dp"            android:layout_marginEnd="30dp"            android:background="@android:color/transparent"            android:src="@drawable/ic_account"            app:layout_constraintBottom_toTopOf="@+id/textView3"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="parent"            tools:ignore="ContentDescription" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.constraintlayout.widget.ConstraintLayout>